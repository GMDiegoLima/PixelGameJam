config_version=5

[application]

config/name="Otter Destruction"
config/description="Otter Destruction is a game about a otter trying to survive the humans destroying the forest."
config/version="1.0.0"
run/main_scene="res://Assets/UI/menu/menu.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
boot_splash/show_image=false
config/icon="res://Assets/Items/axe.png"

[audio]

buses/default_bus_layout="res://Audio/default_bus_layout.tres"

[autoload]

BgMenuMusic="*res://Audio/bg_menu_music.tscn"
BgGameIntro="*res://Audio/bg_game_intro.tscn"
BgGameLoop="*res://Audio/bg_game_loop.tscn"
BgSucessMusic="*res://Audio/bg_sucess_music.tscn"
DialogManager="*res://Assets/UI/DialogManager.gd"

[debug]

